#include "main.h"
#include <stdio.h>
/**
 * print_alphabet - prints alphabets
 * Description: program prints alphabets
 * Return: Always 0 (Success)
 */
void print_alphabet_x10(void)
{
	int c = 0;
	char a = 'a';

	for(;c < 10; c++)
	{
		a = 'a';
		while (a <= 'z')
		{
			putchar(a);
			a++;
		}
		putchar('\n');
	}
}

